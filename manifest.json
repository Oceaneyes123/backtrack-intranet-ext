{
  "manifest_version": 3,
  "name": "Backtrack Intranet",
  "version": "1.1.0",
  "description": "Backtrack Intranet",
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_title": "Backtrack Intranet",
    "default_popup": "popup.html"
  },
  "permissions": ["storage"],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  }
}
